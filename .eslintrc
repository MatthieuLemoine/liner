{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "global-require": 0,
    "no-unused-expressions": 0,
    "no-console": 0,
    "function-paren-newline": 0,
    "import/no-dynamic-require": 0,
  }
}
